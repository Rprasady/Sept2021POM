            steps{
                echo("dev deployment")
            }
            
        }
        stage("Deploy to QA"){
            steps{
                echo("qa deployment")
            }
            
        }
        stage("Run Automation Reg Test"){
            steps{
                echo("Run Automation to Test")
            }
            
        }
        stage("Deploy to Stage"){
            steps{
                echo("Stage deployment")
            }
            
        }
        stage("Deploy to Prod"){
            steps{
                echo("prod deployment")
            }
            
        }
    }
}